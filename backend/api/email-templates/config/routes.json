{
	"routes": [
		{
			"method": "GET",
			"path": "/email-templates",
			"handler": "email-templates.find",
			"config": {
				"policies": ["plugins::users-permissions.isAuthenticated"]
			}
		},
		{
			"method": "GET",
			"path": "/email-templates/count",
			"handler": "email-templates.count",
			"config": {
				"policies": ["plugins::users-permissions.isAuthenticated"]
			}
		},
		{
			"method": "GET",
			"path": "/email-templates/:id",
			"handler": "email-templates.findOne",
			"config": {
				"policies": ["plugins::users-permissions.isAuthenticated"]
			}
		},
		{
			"method": "POST",
			"path": "/email-templates",
			"handler": "email-templates.create",
			"config": {
				"policies": ["plugins::users-permissions.isAuthenticated"]
			}
		},
		{
			"method": "PUT",
			"path": "/email-templates/:id",
			"handler": "email-templates.update",
			"config": {
				"policies": ["plugins::users-permissions.isAuthenticated"]
			}
		},
		{
			"method": "DELETE",
			"path": "/email-templates/:id",
			"handler": "email-templates.delete",
			"config": {
				"policies": ["plugins::users-permissions.isAuthenticated"]
			}
		}
	]
}
