{
    "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 4,
    "generator-cli": {
        "version": "6.6.0",
        "generators": {
            "default": {
                "generatorName": "typescript-axios",
                "output": "#{cwd}/src/.openapi/",
                "glob": "../backend/src/extensions/documentation/documentation/1.0.0/full_documentation_clean.json",
                "skipValidateSpec": true,
                "additionalProperties": {
                    "ngVersion": "16.1.2",
                    "typescriptThreePlus": true,
                    "supportsES6": true,
                    "hateoas": true,
                    "withInterfaces": true,
                    "useSingleRequestParameter": true
                }
            }
        }
    }
}
